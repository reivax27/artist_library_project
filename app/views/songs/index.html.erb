<h2>All Songs</h2>

<div class="card" style="width: 30rem;">
  <ul class="list-group list-group-flush">
    <% @songs.each do |song| %>
      <li class="list-group-item list-group-item-primary">The song "<%= link_to song.title, song %>" is by <%= link_to song.artist.name, song.artist %></li>
    <% end %>
  </ul>
</div>
